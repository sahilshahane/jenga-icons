{
  "name": "jenga-icons",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "changeset": "changeset",
    "clean": "turbo clean && rm -rf node_modules",
    "generate-optimized": "pnpm --filter generate-optimized build && pnpm --filter generate-optimized write-icons ../../assets",
    "generate-react": "pnpm --filter generate-jengaicons-react write-icons --assets ../../optimized --root-path ../../packages/jengaicons-react --output-folder-name icons",
    "release": "changeset publish",
    "release:dev": "changeset publish --tag dev",
    "test": "turbo test",
    "version": "changeset version",
    "version:dev": "changeset version --snapshot dev",
    "example:build": "pnpm --filter next-ts-example build",
    "example:dev": "pnpm --filter next-ts-example dev",
    "example:start": "pnpm --filter next-ts-example start"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.0",
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@svgr/cli": "^6.2.1",
    "@types/node": "^18.11.18",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "turbo": "^1.7.2",
    "typescript": "^4.6.3"
  }
}
